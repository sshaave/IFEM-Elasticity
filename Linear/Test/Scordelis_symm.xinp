<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Scordelis-Lo roof (1/4), subjected to gravity load.
	 Quadratic spline Kirchhoff-Love thin shell elements. !-->

<simulation>

	<!-- General - gemoetry definitions !-->
	<geometry>
		<patchfile>Scordelis_symm.g2</patchfile>
		<raiseorder patch="1" u ="2" v="3"/>
    		<refine patch="1" u="10" v="10"/>
    		<topologysets>
		  	<set name="symmZY" type="edge">
		    	<item patch="1">1</item>
		  	</set>
		  	<set name="symmXZ" type="edge">
		    	<item patch="1">4</item>
			</set>
		  	<set name="fixedend" type="edge">
		    	<item patch="1">3</item>
			</set>
		</topologysets>
	</geometry>

  	<!-- General - Gauss quadrature scheme !-->
  	<discretization>
   		<nGauss>5</nGauss>
 	</discretization>

	<!-- General - boundary conditions !-->
  	<boundaryconditions>
	    	<dirichlet set="fixedend" comp="13"/>
	    	<dirichlet set="symmZY" comp="1"/>
	    	<dirichlet set="symmZY" comp="2000"/>
	    	<dirichlet set="symmZY" comp="3000"/>
	    	<dirichlet set="symmXZ" comp="1000"/>
	    	<dirichlet set="symmXZ" comp="2"/>
	    	<dirichlet set="symmXZ" comp="3000"/>
  	</boundaryconditions>

  	<!-- Problem specific block !-->
  	<KirchhoffLove>
    		<gravity g="-1.0"/>
    		<isotropic E="4.32e8" nu="0.0" rho="360.0" thickness="0.25"/>
  	</KirchhoffLove>

	<!-- General - point result output !-->
	<postprocessing>
		<resultpoints>
		<point patch="1" u="1.0" v="1.0"/>
	</resultpoints>
	</postprocessing>

</simulation>
