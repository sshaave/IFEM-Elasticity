<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Pinched cylinder (1/8), subjected to point loads.
     Quartic spline Kirchhoff-Love thin shell elements. !-->

<simulation>

  <!-- General - gemoetry definitions !-->
  <geometry>
  <patchfile>PinchedCylinder_symm.g2</patchfile>
  <raiseorder patch="1" u ="2" v="3"/>
  <refine patch="1" u="3" v="3"/>
    <topologysets>
      <set name="symmYZ" type="edge">
        <item patch="1">1</item>
      </set>
      <set name="symmXZ" type="edge">
        <item patch="1">4</item>
      </set>
      <set name="symmXY" type="edge">
        <item patch="1">2</item>
      </set>
      <set name="fixedend" type="edge">
        <item patch="1">3</item>
      </set>
    </topologysets>
  </geometry>

  <!-- General - Gauss quadrature scheme !-->
  <discretization>
    <nGauss default="0"/>
  </discretization>

  <!-- General - boundary conditions !-->
  <boundaryconditions>
    <dirichlet set="fixedend" comp="13"/>
    <dirichlet set="symmYZ" comp="1"/>
    <dirichlet set="symmYZ" comp="2000"/>
    <dirichlet set="symmYZ" comp="3000"/>
    <dirichlet set="symmXZ" comp="1000"/>
    <dirichlet set="symmXZ" comp="2"/>
    <dirichlet set="symmXZ" comp="3000"/>
    <dirichlet set="symmXY" comp="3"/>
    <dirichlet set="symmXY" comp="2000"/>
    <dirichlet set="symmXY" comp="1000"/>
  </boundaryconditions>

  <!-- Problem specific block !-->
  <KirchhoffLove>
    <isotropic E="3.00e6" nu="0.3" thickness="3.0"/>
    <pointload patch="1" xi="0.0" eta="1.0" direction="3">-0.25</pointload>
  </KirchhoffLove>

  <!-- General - point result output !-->
  <postprocessing>
    <resultpoints>
      <point patch="1" u="1.0" v="1.0"/>
    </resultpoints>
  </postprocessing>

</simulation>
