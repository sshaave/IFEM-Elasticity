<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Hemispherical shell, subjected to point load.
     Quartic spline Kirchhoff-Love thin shell elements. !-->

<simulation>

  <!-- General - geometry definitions !-->
  <geometry>
  <patchfile>Hemisphere_symm.g2</patchfile>
  <raiseorder patch="1" u="3" v="3"/>
  <refine patch="1" u="15" v="15"/>
  <topologysets>
  	<set name="fixed" type="edge">
    	<item patch="1">1</item>
    </set>
    <set name="symmXZ" type="edge">
      <item patch="1">3</item>
    </set>
    <set name="symmXY" type="edge">
      <item patch="1">4</item>
    </set>
  </topologysets>
  </geometry>

  <!-- General - Gauss quadrature scheme !-->
  <discretization>
    <nGauss>4</nGauss>
  </discretization>

  <!-- General - boundary conditions !-->
  <boundaryconditions>
    <dirichlet set="fixed" comp="123"/>
    <dirichlet set="symmXZ" comp="1000"/>
    <dirichlet set="symmXZ" comp="2"/>
    <dirichlet set="symmXZ" comp="3000"/>
    <dirichlet set="symmXY" comp="1000"/>
    <dirichlet set="symmXY" comp="2000"/>
    <dirichlet set="symmXY" comp="3"/>
  </boundaryconditions>

  <!-- Problem specific block !-->
  <KirchhoffLove>
    <isotropic E="6.825e7" nu="0.3" thickness="0.04"/>
    <pointload patch="1" xi="1.0" eta="0.0" direction="3">1.0</pointload>
    <pointload patch="1" xi="1.0" eta="1.0" direction="2">-1.0</pointload>
  </KirchhoffLove>

  <!-- General - point result output !-->
  <postprocessing>
    <resultpoints>
      <point patch="1" u="1.0" v="0.0"/>
      <point patch="1" u="1.0" v="1.0"/>
    </resultpoints>
  </postprocessing>

</simulation>
