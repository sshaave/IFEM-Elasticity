<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Pinched cylinder subjected to point load.
     Quartic spline Kirchhoff-Love thin shell elements. !-->

<simulation>

  <!-- General - geometry definitions !-->
  <geometry>
    <patchfile>Pinched-cylinder.g2</patchfile>
    <raiseorder patch="1" u="2" v="3"/>
    <refine patch="1" u="3" v="3"/>
    <topologysets>
      <set name="ends" type="edge">
        <item patch="1">34</item>
      </set>
      <set name="fixed" type="vertex">
        <item patch="1">1</item>
      </set>
    </topologysets>
  </geometry>

  <!-- General - Gauss quadrature scheme !-->
  <discretization>
    <nGauss>3</nGauss>
  </discretization>

  <!-- General - boundary conditions !-->
  <boundaryconditions>
    <dirichlet set="ends" comp="13"/>
    <dirichlet set="fixed" comp="2"/>
  </boundaryconditions>

  <!-- Problem specific block !-->
  <KirchhoffLove>
    <isotropic E="3.0e6" nu="0.3" thickness="0.3"/>
    <pointload patch="1" xi="0.0" eta="0.5" direction="3">-1</pointload>
    <pointload patch="1" xi="0.5" eta="0.5" direction="3">1</pointload>
  </KirchhoffLove>

  <!-- General - point result output !-->
  <postprocessing>
    <resultpoints>
      <point patch="1" u="0.0" v="0.5"/>
      <point patch="1" u="1.0" v="0.5"/>
    </resultpoints>
  </postprocessing>

</simulation>
